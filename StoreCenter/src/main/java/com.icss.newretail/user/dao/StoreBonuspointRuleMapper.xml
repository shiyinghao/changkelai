<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.icss.newretail.user.dao.StoreBonuspointRuleMapper">

    <select id="queryStoreBonuspointOne" resultType="com.icss.newretail.model.StoreBonuspointRuleDTO">
        select uuid, consume_rate,
	back_bind_rate,
	back_nobind_rate,
	share_score  from  t_user_store_bonuspoint_rule  where 1=1    limit 1
    </select>
</mapper>
