<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.icss.newretail.user.dao.WechatOrgCardMapper">
	<select id="getWechatOrgCard" parameterType="Map" resultType="com.icss.newretail.user.entity.WechatOrgCard">
		SELECT * FROM t_user_wechat_org_card
		WHERE status=1 AND corp_id = #{corpId} AND org_seq = #{orgSeq} AND card_id = #{cardId}
	</select>
</mapper>
